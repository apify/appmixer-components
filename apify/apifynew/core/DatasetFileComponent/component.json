{
    "name": "apify.apifynew.core.DatasetFileComponent",
    "author": "Apify team",
    "description": "This component can export Apify dataset as a single file.",
    "private": false,
    "quota": {
        "manager": "apify:apify",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "actorData",
            "schema": {
                "properties": {
                    "resources": {
                        "type": "object",
                        "properties": {
                            "defaultDatasetId": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "datasetFile",
            "options": [
                {
                    "label": "Dataset file",
                    "value": "fileId"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}